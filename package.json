{
  "name": "test-data-generator",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "description": "A simple web app for generating test data.",
  "main": "index.js",
  "scripts": {
    "start": "vite",
    "clean": "rimraf dist playwright-report test-results",
    "format": "prettier --write \"./**/*.{css,html,js,json,md,prettierrc,yaml,yml}\"",
    "lint": "eslint . --fix",
    "build": "vite build",
    "build-cloudflare-pages": "vite build --base=/",
    "preview": "vite preview",
    "test": "vitest run",
    "e2e": "playwright test",
    "e2e-ui": "playwright test --ui",
    "e2e-update-snapshots": "playwright test --update-snapshots",
    "prepare": "husky"
  },
  "keywords": [
    "test data",
    "generator"
  ],
  "author": "luth1um",
  "license": "MIT",
  "dependencies": {
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@eslint/css": "0.13.0",
    "@eslint/js": "9.38.0",
    "@playwright/test": "1.56.1",
    "eslint": "9.38.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.4",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "vite": "7.1.12",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "*.{css,html,js,json,md,prettierrc,yaml,yml}": "prettier --write"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
